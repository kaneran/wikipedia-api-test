{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "eslint:recommended",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-unused-vars": "error",
        "no-undef": "off",
        "no-const-assign": "error",
        "no-dupe-else-if": "error",
        "no-unreachable-loop": "warn",
        "max-params": "warn",
        "max-len": ["error", {"code": 150}],
        "quotes": ["error", "double"],
        "semi": ["error", "never"]
    }
}
